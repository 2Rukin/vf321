@Override
public Expression<PfLoanFundsRqRegistryItemBo> getProjections(List<RegistryCodes> fields) {
    return Projections.bean(
        PfLoanFundsRqRegistryItemBo.class,
        paymentAnalyticEntity.id.as("id"),
        paymentAnalyticEntity.clientId.as("clientId"),
        paymentAnalyticEntity.paymentDocumentId.as("paymentDocumentId"),
        fields.contains(RegistryCodes.NUMBER) ? paymentAnalyticEntity.number.as("number") : null,
        fields.contains(RegistryCodes.LOAN_FUNDS_REQUEST_STATUS) ? paymentAnalyticEntity.status.as("status") : null,
        fields.contains(RegistryCodes.DATE) ? paymentAnalyticEntity.date.as("date") : null,
        fields.contains(RegistryCodes.FUNDS_TYPE) ? paymentAnalyticEntity.fundsType.as("fundsType") : null,
        fields.contains(RegistryCodes.CREDIT_AGREEMENT) ? paymentAnalyticEntity.creditAgreementNumber.as("creditAgreementNumber") : null,
        fields.contains(RegistryCodes.CREDIT_AGREEMENT) ? paymentAnalyticEntity.creditAgreementDate.as("creditAgreementDate") : null,
        fields.contains(RegistryCodes.PAYER_ACCOUNT) ? paymentAnalyticEntity.payerAccount.as("payerAccount") : null,
        fields.contains(RegistryCodes.OB_ACCOUNT_FLAG) ? paymentAnalyticEntity.obcAccountFlag.as("obcAccountFlag") : null,
        fields.contains(RegistryCodes.TRANCHE_ISSUE_DATE) ? paymentAnalyticEntity.trancheIssueDate.as("trancheIssueDate") : null,
        fields.contains(RegistryCodes.AMOUNT) ? paymentAnalyticEntity.amount.as("amount") : null,
        fields.contains(RegistryCodes.PAYMENT_PURPOSE) ? paymentAnalyticEntity.paymentPurpose.as("paymentPurpose") : null,
        fields.contains(RegistryCodes.RECIPIENT_NAME) ? paymentAnalyticEntity.recipientName.as("recipientName") : null,
        fields.contains(RegistryCodes.RECIPIENT_ACCOUNT) ? paymentAnalyticEntity.recipientAccount.as("recipientAccount") : null,
        fields.contains(RegistryCodes.DVRU) ? paymentAnalyticEntity.dvruNumber.as("dvruNumber") : null,
        fields.contains(RegistryCodes.DVRU) ? paymentAnalyticEntity.dvruDate.as("dvruDate") : null,
        fields.contains(RegistryCodes.PAYMENT_TYPE) ? paymentAnalyticEntity.paymentType.as("paymentType") : null,
        fields.contains(RegistryCodes.PAYMENT_OBJECT) ? Projections.list(
            Projections.bean(
                PfLoanFundsRqRegistryItemBo.PaymentObjectEntityBo.class,
                paymentAnalyticEntity.paymentObjects.any().name.as("name"),
                fields.contains(RegistryCodes.PAYMENT_OBJECT) ? paymentAnalyticEntity.paymentObjects.any().projectName.as("projectName") : null,
                Projections.bean(
                    PfLoanFundsRqRegistryItemBo.PaymentObjectSsrEntityBo.class,
                    Projections.list(
                        Projections.bean(
                            PfLoanFundsRqRegistryItemBo.PaymentSsrArticleEntityBo.class,
                            fields.contains(RegistryCodes.SSR_ARTICLE) ? paymentAnalyticEntity.paymentObjects.any()
                                .paymentObjectSsr.paymentSsrs.any().code.as("code") : null
                        )
                    ).as("paymentSsrs")
                ).as("paymentObjectSsr")
            )
        ).as("paymentObjects") : null
    );
}
