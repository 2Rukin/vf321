for (StatusCountDto expectedDto : expected) {
    // Проверяем, что результат содержит элемент, соответствующий ожидаемому DTO
    assertTrue(result.stream().anyMatch(tuple -> {
        String status = tuple.get(qPaymentAnalyticEntity.status);
        Long count = tuple.get(qPaymentAnalyticEntity.id.count());

        return expectedDto.getStatus().equals(status) && 
               expectedDto.getCount().equals(count.intValue()); // Приводим Long count к Integer для сравнения
    }), "Результат должен содержать " + expectedDto);
}
